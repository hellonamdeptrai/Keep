<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Keep" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/gray_app_keep</item>
        <item name="colorPrimaryVariant">@color/black_app_keep</item>
        <item name="colorOnPrimary">@color/white_dark_app_keep</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/gray_app_keep</item>
        <item name="colorSecondaryVariant">@color/black_app_keep</item>
        <item name="colorOnSecondary">@color/white_dark_app_keep</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Keep.PopupOverlay" parent="ThemeOverlay.AppCompat.DayNight.ActionBar" />
</resources>